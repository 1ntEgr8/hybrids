{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "no-debugger": 0,
    "no-param-reassign": 0,
    "no-new-func": 0,
    "new-cap": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["test/**/*.js"]
    }]
  },
  "globals": {
    "hybrid": true
  }
}
